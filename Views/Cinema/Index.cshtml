@model List<Cinema>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col"></th>
            
        </tr>
    </thead>
    <tbody class="table-group-divider">
        @foreach(var item in Model)
        {
            <tr>
                <td scope="row">@item.Name</td>
                <td><a href="/Cinema/DisplayMovies?Id=@item.Id">All Movies</a></td>
            </tr>
        }
        
       
    </tbody>
</table>
